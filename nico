public class nico : barragebase{

	override void move1 ()
	{

	}
	override void move2 ()
	{

	}
	override void move3 ()
	{

	}
	
	override IEnumerator behaviorControl ()
	{
		if(lv==3)
		{

		}
		else if(lv==2)
		{
			this.move1();
		}
		else if(lv==1)
		{
			this.move2();
		}
		else if(lv==0)
		{
			this.move3();
		}

	}




}
